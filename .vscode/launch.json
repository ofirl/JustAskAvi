    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Remote",
            "address": "just-ask-avi.herokuapp.com",
            "port": 443,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "https://git.heroku.com/just-ask-avi.git"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}\\index.js"
        }
    ]
}